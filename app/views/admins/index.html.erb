<p id="notice"><%= notice %></p>

<h3>Administrator List</h3>

<table class="table table-striped">
  <thead>
    <tr>
      <th>E-mail</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admins.each do |admin| %>
      <tr>
        <td><%= admin.email %></td>
        <td><%= link_to "Show", admin %></td>
        <td><%= link_to "Edit", edit_admin_path(admin) %></td>
        <td><%= link_to "Delete", admin, method: :delete, data: { confirm:
          "Are you sure you want to delete?" } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to "New Admin", new_admin_path %>
