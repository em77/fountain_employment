<tr class="table_row">
  <td><%= found.job_title %></td>
  <td><%= found.description %></td>
  <td><%= decimalize(found.wage) %></td>
  <td><%= found.working_hours_desc %></td>
  <td><%= decimalize(found.hours_weekly) %></td>
  <td><%= found.shift %></td>
  <td><%= found.employment_type %></td>
  <td><%= found.start_date %></td>
  <td><%= found.est_end_date %></td>
  <td><%= found.te_placement_manager %></td>
  <% if logged_in? %>
    <td><%= found.member_working %></td>
    <td>
      <%= link_to "Show", found %><br>
      <%= link_to "Edit", edit_listing_path(found) %><br>
      <%= link_to "Delete", found, method: :delete, data: { confirm:
        "Are you sure you want to delete?" } %>
    </td>
  <% else %>
    <% case found.member_working %>
    <% when nil %>
      <td>Yes</td>
    <% else %>
      <td>No</td>
    <% end %>
  <% end %>
</tr>

<% if false %>
  <tr class="table_row">
    <td><%= found.name %></td>
    <td><%= found.description %></td>
    <% if logged_in? %>
      <td>
        <%= link_to "Show", found %><br>
        <%= link_to "Edit", edit_company_path(found) %><br>
        <%= link_to "Delete", found, method: :delete, data: { confirm:
          "Are you sure you want to delete this company?" } %><br>
        <%= link_to "Delete All Listings", destroy_all_listings_companies_path(id: found.id), method: :delete, data: { confirm: "Are you sure you want to delete all listings for this company?" } %>
      </td>
    <% end %>
  </tr>
<% end %>
